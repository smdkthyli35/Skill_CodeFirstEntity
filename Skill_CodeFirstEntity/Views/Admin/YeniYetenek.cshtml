@model Skill_CodeFirstEntity.Models.Siniflar.YETENEKLER

@{
    ViewBag.Title = "YeniYetenek";
    Layout = "~/Views/Shared/_LayoutNavbar.cshtml";
}

<form class="form-group" method="post">
    <div style="margin-bottom:15px">
        <label>Yetenek Açıklama</label>
        @Html.TextBoxFor(x => x.ACIKLAMA, new { @class = "form-control" })
    </div>

    <div>
        <label>Yetenek Değeri</label>
        @Html.TextBoxFor(x => x.DEGER, new { @class = "form-control" })
    </div>

    <button style="margin-top:15px" type="submit" class="btn btn-success">Kaydet</button>
</form>
