@using Skill_CodeFirstEntity.Models.Siniflar
@model List<YETENEKLER>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutNavbar.cshtml";
}

<table class="table table-bordered">

    <tr>
        <th>ID</th>
        <th>YETENEK</th>
        <th>DEĞER</th>
        <th>SİL</th>
        <th>GÜNCELLE</th>
    </tr>

    <tbody>
        @foreach (var x in Model)
        {
            <tr>
                <td>@x.ID</td>
                <td>@x.ACIKLAMA</td>
                <td>@x.DEGER</td>
                <td> <a class="btn btn-danger" href="/Admin/YetenekSil/@x.ID">SİL</a> </td>
                <td> <a class="btn btn-primary" href="/Admin/YetenekGetir/@x.ID">GÜNCELLE</a> </td>
            </tr>
        }
    </tbody>
</table>
